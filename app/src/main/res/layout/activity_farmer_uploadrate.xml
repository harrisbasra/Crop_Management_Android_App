<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/fullscreenBackgroundColor"
    android:backgroundTint="#D16969"
    android:theme="@style/ThemeOverlay.AgriTech.FullscreenContainer"
    tools:context=".FarmerUploadRate">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->
    <TextView
        android:id="@+id/fullscreen_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EDCFCF"
        android:gravity="center"
        android:keepScreenOn="false"
        android:textColor="?attr/fullscreenTextColor"
        android:textSize="50sp"
        android:textStyle="bold" />

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <LinearLayout
            android:id="@+id/fullscreen_content_controls"
            style="?attr/fullscreenBackgroundColor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:orientation="horizontal"
            tools:ignore="UselessParent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#646F78">

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingActionButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:contentDescription="@string/dummy_button"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6"
                app:layout_constraintVertical_bias="0.504"
                app:srcCompat="@android:drawable/ic_input_add" />

            <EditText
                android:id="@+id/textView5"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Quantity:"
                android:inputType="textShortMessage"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.32" />

            <EditText
                android:id="@+id/textView4"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Rainfall:"
                android:inputType="textShortMessage"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.633" />

            <EditText
                android:id="@+id/textView9"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Pesticides:"
                android:inputType="textShortMessage"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.734" />

            <EditText
                android:id="@+id/textView2"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Location:"
                android:inputType="textShortMessage"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.526" />

            <EditText
                android:id="@+id/textView8"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Temperature:"
                android:inputType="datetime"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.424" />

            <EditText
                android:id="@+id/textView6"
                android:layout_width="333dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Offer A Price:"
                android:inputType="number"
                android:paddingLeft="10sp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.525"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.837" />

            <Spinner
                android:id="@+id/spinnerA"
                android:layout_width="329dp"
                android:layout_height="60dp"
                android:background="@drawable/roundist"
                android:gravity="center_vertical"
                android:hint="Quality:"
                android:inputType="textShortMessage"
                android:paddingLeft="10sp"
                android:spinnerMode="dialog"
                android:textSize="24sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.214" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="333dp"
                android:layout_height="102dp"
                android:background="@drawable/roundbb"
                android:gravity="center"
                android:text="Upload Crops"
                android:textSize="34sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.023" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>

</FrameLayout>